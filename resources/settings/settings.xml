            <listEntry value="2">@Strings.ShowBatteryConsumed</listEntry>
            <listEntry value="3">@Strings.ShowModeNumber</listEntry>
            <listEntry value="4">@Strings.ShowModeName</listEntry>
            <listEntry value="5">@Strings.ShowModeLetter</listEntry>
            <listEntry value="6">@Strings.ShowGear</listEntry>
            <listEntry value="7">@Strings.ShowCadence</listEntry>
            <listEntry value="8">@Strings.ShowAssistanceLevel</listEntry>
            <listEntry value="9">@Strings.ShowCurrentSpeed</listEntry>
